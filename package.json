{
  "name": "chronosai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:security": "jest --testPathPattern=__tests__/security/",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "playwright test",
    "test:prepare-data": "tsx scripts/prepare-test-data.ts",
    "test:cleanup-data": "tsx scripts/prepare-test-data.ts --cleanup",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "load-test:generate-data": "tsx load-tests/scripts/generate-test-data.ts",
    "load-test:cleanup": "tsx load-tests/scripts/generate-test-data.ts --cleanup",
    "load-test:monitor": "tsx load-tests/scripts/monitor-metrics.ts",
    "load-test:chat": "artillery run --config load-tests/artillery/artillery.config.yml load-tests/artillery/scenarios/chat-load.yml",
    "load-test:video": "artillery run --config load-tests/artillery/artillery.config.yml load-tests/artillery/scenarios/video-upload-load.yml",
    "load-test:auth": "artillery run --config load-tests/artillery/artillery.config.yml load-tests/artillery/scenarios/auth-load.yml",
    "load-test:database": "artillery run --config load-tests/artillery/artillery.config.yml load-tests/artillery/scenarios/database-load.yml",
    "load-test:rate-limits": "artillery run --config load-tests/artillery/artillery.config.yml load-tests/artillery/scenarios/api-rate-limit-test.yml",
    "load-test:quick": "artillery quick --count 10 --num 30 http://localhost:3000/api/health",
    "load-test:verify": "artillery validate load-tests/artillery/scenarios/chat-load.yml",
    "load-test:all": "npm run load-test:chat && npm run load-test:video && npm run load-test:auth && npm run load-test:database"
  },
  "dependencies": {
